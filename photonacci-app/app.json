{
  "expo": {
    "name": "Photonacci",
    "slug": "Photonacci",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#CACFD2"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
