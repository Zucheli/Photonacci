      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zucheliinc.photonacci",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3702483996895342~2741557686"
      }
    },
    "android": {
      "package": "com.zucheliinc.photonacci",
      "adaptiveIcon": {
        "backgroundColor": "#CACFD2",
        "foregroundImage": "./assets/icon.png"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3702483996895342~2741557686" 
      }
    }
  }
}
